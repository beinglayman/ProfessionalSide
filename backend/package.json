{
  "name": "inchronicle-backend",
  "version": "1.0.5",
  "description": "Backend API for InChronicle professional journaling platform",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "npm run db:generate && tsc",
    "build:production": "npm run db:generate && tsc --noEmitOnError false",
    "start": "node migrate-and-start.js",
    "start:with-migrations": "npm run db:migrate:deploy && npx tsx src/app.ts",
    "start:with-depth-fix": "npm run db:migrate:deploy && (npm run db:seed-reference || echo 'Seeding failed but continuing...') && npm run db:fix-supply-chain && npm run db:ensure-depth-coverage && npx tsx src/app.ts",
    "start:production": "npm run db:migrate:deploy && (npm run db:seed-reference || echo 'Seeding failed but continuing...') && npx tsx src/app.ts",
    "health-check": "curl -f http://localhost:3002/health",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-reference": "tsx prisma/seed-reference-data.ts",
    "db:seed-complete": "npm run db:seed-reference && node import-skill-benchmarks.js",
    "db:add-design-skills": "node scripts/add-design-skills.js",
    "db:add-missing-categories": "node scripts/add-missing-work-categories.js",
    "db:seed-billing": "tsx prisma/seed-billing.ts",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "typescript",
    "express",
    "prisma",
    "postgresql",
    "jwt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@prisma/client": "^5.7.1",
    "@tanstack/react-query": "^5.81.5",
    "@types/handlebars": "^4.0.40",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.9.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "googleapis": "^164.1.0",
    "graphlib": "^2.1.8",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "neverthrow": "^8.2.0",
    "node-cron": "^4.2.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "razorpay": "^2.9.6",
    "react-icons": "^5.5.0",
    "redis": "^4.6.12",
    "serve": "^14.2.5",
    "socket.io": "^4.7.4",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/canvas-confetti": "^1.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/graphlib": "^2.1.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.14.0",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "prisma": "^5.7.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
